<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="SemiWhiteColor">#E1E1DE</Color>
    <Color x:Key="SemiDarkColor">#363941</Color>
    <SolidColorBrush x:Key="SemiWhite" Color="{StaticResource SemiWhiteColor}"/>
    <SolidColorBrush x:Key="SemiBlack" Color="{StaticResource SemiDarkColor}"/>
</ResourceDictionary>