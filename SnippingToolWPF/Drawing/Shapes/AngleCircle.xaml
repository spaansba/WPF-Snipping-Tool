<UserControl x:Class="SnippingToolWPF.AngleCircle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:snippingToolWpf="clr-namespace:SnippingToolWPF"
             mc:Ignorable="d" Width="300" Height="300"
             DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <Grid>
        <!-- <Path Data="M 100,20 L 100,0 A 100,100 90 0 1 200,100 L 180,100 M 100,20 A 80,80 90 0 1 180,100" Fill="Black"/> -->
        <snippingToolWpf:DonutPiece Stroke="Red" Fill="Black" />
        <TextBlock Text="{Binding AngleTextBox}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>

</UserControl>